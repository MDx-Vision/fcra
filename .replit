modules = ["python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["stripe:1.0.0", "twilio:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "freetype", "glibcLocales", "libiconv", "libxcrypt", "openssl", "pkg-config", "poppler-utils", "rustc", "dbus", "gtk3", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXrandr", "xorg.libXcursor", "xorg.libXi", "xorg.libXtst", "xorg.libXScrnSaver", "nss", "alsa-lib", "mesa", "libnotify", "xvfb-run", "cypress", "libxkbcommon", "pango", "cairo", "gdk-pixbuf", "atk", "at-spi2-atk", "at-spi2-core", "libdrm", "expat", "cups", "systemd", "xorg.libxkbfile", "imagemagick_light"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
